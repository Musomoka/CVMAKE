<div class= 'profile section'>


    <h1>Profiles</h1>

    <table>
    <thead>
        <tr>
        <th>Firstname</th>
        <th>Surname</th>
        <th>Gender</th>
        <th colspan="3"></th>
        </tr>
    </thead>

    <tbody>
        <% @profiles.each do |profile| %>
        <tr>
            <td><%= profile.firstname %></td>
            <td><%= profile.surname %></td>
            <td><%= profile.gender %></td>
            
        </tr>
        <% end %>
    </tbody>
    </table>

    <br>

    <%= link_to 'New Profile', new_profile_path %>


</div>
<p id="notice"><%= notice %></p>

<h1>Addresses</h1>
<%if @addresses.blank? %>
<h3>Please add an address</h3>
<%= link_to 'Add address', new_address_path %>
<%else%>
<table>
  <thead>
    <tr>
      <th>Addresstype</th>
      <th>Addressdetail</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @addresses.each do |address| %>
      <tr>
        <td><%= address.addressType %></td>
        <td><%= address.addressDetail %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


<%end%>